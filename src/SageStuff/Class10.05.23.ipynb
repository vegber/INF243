   "source": [
    "L0 = -0.1\n",
    "L1 = 0.5\n",
    "L2 = -0.8\n",
    "\n",
    "def addLLR(L1, L2): \n",
    "    val = (1+exp(L1+L2)) / (exp(L1) + exp(L2))\n",
    "    return log(val).n(3)\n",
    "\n",
    "\n",
    "addLLR(L1, L2)"
   ]
  },
