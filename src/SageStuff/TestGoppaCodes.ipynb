   "source": [
    "F = GF(2^4)\n",
    "R.<x> = F[]\n",
    "g = x^7 + x + 1\n",
    "L = [a for a in F if g(a)]\n",
    "C = codes.GoppaCode(g, L)\n",
    "D = C.decoder()"
   ]
